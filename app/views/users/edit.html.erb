<h1>User#edit</h1>
<p>Find me in app/views/user/edit.html.erb</p>
 <%= form_for :user, url: edit_user_path do |f| %>


 	<p>
 		<%= f.label :privacy %> 
 		<%= f.check_box :privacy %>
	<p> 
		<%= f.submit 'Change Privacy' %>
	<p>
		<%= f.label :password %><br>
		<%= f.text_field :new_password %>
	</p>
	<p>
		<%= f.label :password_confirmation %><br>
		<%= f.text_field :new_password_confirmation %>
	</p>

	<p>
		<%= f.submit 'Change Password' %>
	</p>
	<% end %> 